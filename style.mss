Map {
  background-color: @land-color;
}

@water-color: #aad3df;
@land-color: #f2efe9;

@standard-halo-radius: 1;
@standard-halo-fill: rgba(255,255,255,0.6);

@areasize0: 750;
@areasize1: @areasize0 * 4;
@areasize2: @areasize1 * 4;
@areasize3: @areasize2 * 4;
@areasize4: @areasize3 * 4;
@areasize5: @areasize4 * 4;
